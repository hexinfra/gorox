httpxServer "hs1" {
	.webapps             = ("foo", "bar")
	.address             = string,required
	.tlsMode             = false
	.readTimeout         = 60s
	.writeTimeout        = 60s
	.numGates            = runtime.NumCPU()
	.maxConnsPerGate     = 10000
	.maxContentSize      = 1T
	.recvTimeout         = 120s
	.sendTimeout         = 120s
	.forceScheme         = ""
	.alignScheme         = true
	.saveContentFilesDir = %tmpDir + "/web/servers/hs1"
}
