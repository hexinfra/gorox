http1Backend "h1b1" {
	.idleTimeout = 2s
	.maxLifetime = 1m
	.maxCumulativeStreamsPerConn = 1000
	.saveContentFilesDir = %tmpDir + "/web/backends/h1b"
	.maxContentSizeAllowed = 1T
	.recvTimeout = 0s
	.sendTimeout = 0s
	.balancer = "roundRobin"
	node {
		.tlsMode = false
		.address = "127.0.0.1:3080"
		.dialTimeout = 10s
		.readTimeout = 30s
		.writeTimeout = 30s
		.weight = 9
		.keepAliveConns = 2
	}
	node {
		.tlsMode = false
		.address = "127.0.0.1:3081"
		.dialTimeout = 10s
		.readTimeout = 30s
		.writeTimeout = 30s
		.weight = 8
		.keepAliveConns = 3
	}
}
