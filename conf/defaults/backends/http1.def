http1Backend "h1b1" {
	.tlsMode = false
	.dialTimeout = 10s
	.writeTimeout = 30s
	.readTimeout = 30s
	.aliveTimeout = 5s
	.nodes = (
		["address": "127.0.0.1:3080", "weight": 9, "keepConns": 2],
		["address": "127.0.0.1:3080", "weight": 8, "keepConns": 3],
	)
	.maxStreamsPerConn = 1000
	.saveContentFilesDir = %tmpsDir + "/web/backends/h1b"
	.maxContentSize = 1T
	.sendTimeout = 60s
	.recvTimeout = 60s
	.balancer = "roundRobin"
}
