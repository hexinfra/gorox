httpxServer "hs1" {
	.forApps             = ("foo", "bar")
	.address             = string,required
	.tlsMode             = false
	.readTimeout         = 60s
	.writeTimeout        = 60s
	.numGates            = runtime.NumCPU()
	.maxConnsPerGate     = 10000
	.maxContentSizeAllowed      = 1T
	.recvTimeout         = 120s
	.sendTimeout         = 120s
	.forceScheme         = ""
	.adjustScheme        = true
	.enableHTTP2         = false
	.http2Only           = false
	.saveContentFilesDir = %tmpsDir + "/web/servers/hs1"
}
