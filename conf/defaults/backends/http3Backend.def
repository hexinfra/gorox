http3Backend "h3b1" {
	.tlsMode = false
	.dialTimeout = 10s
	.writeTimeout = 30s
	.readTimeout = 30s
	.aliveTimeout = 5s
	.maxStreamsPerConn = 1000
	.saveContentFilesDir = %tmpsDir + "/web/backends/h3b"
	.maxContentSizeAllowed = 1T
	.sendTimeout = 60s
	.recvTimeout = 60s
	.balancer = "ipHash"
	node {
		.address = "127.0.0.1:3080"
		.weight = 9
		.keepConns = 2
	}
	node {
		.address = "127.0.0.1:3081"
		.weight = 8
		.keepConns = 3
	}
}
