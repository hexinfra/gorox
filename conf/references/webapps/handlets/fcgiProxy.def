fcgiProxy "fp1" {
	.saveContentFilesDir = %tmpsDir + "/web/fcgi/fp1"
	.toBackend = "php-fpm"
	.sendTimeout = 60s
	.recvTimeout = 60s
	.scriptFilename = ""
	.indexFile = "index.php"
	.persistent = true
	.bufferClientContent = true
	.bufferServerContent = true
	.preferUnderscore = true
	.withCacher = "cacher name"
}
