http3Backend "h3b1" {
	.tlsMode = false
	.dialTimeout = 10s
	.writeTimeout = 30s
	.readTimeout = 30s
	.idleTimeout = 2s
	.maxLifetime = 1m
	.maxStreamsPerConn = 1000
	.saveContentFilesDir = %tmpDir + "/web/backends/h3b"
	.maxContentSize = 1T
	.recvTimeout = 0s
	.sendTimeout = 0s
	.balancer = "ipHash"
	node {
		.address = "127.0.0.1:3080"
		.weight = 9
		.keepAliveConns = 2
	}
	node {
		.address = "127.0.0.1:3081"
		.weight = 8
		.keepAliveConns = 3
	}
}
