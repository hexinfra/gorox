http3Backend "h3b1" {
	.idleTimeout = 2s
	.maxLifetime = 1m
	.maxStreamsPerConn = 1000
	.saveContentFilesDir = %tmpDir + "/web/backends/h3b"
	.maxContentSize = 1T
	.recvTimeout = 0s
	.sendTimeout = 0s
	.balancer = "ipHash"
	node {
		.tlsMode = true
		.address = "127.0.0.1:3080"
		.dialTimeout = 10s
		.readTimeout = 30s
		.writeTimeout = 30s
		.weight = 9
		.keepAliveConns = 2
	}
	node {
		.tlsMode = true
		.address = "127.0.0.1:3081"
		.dialTimeout = 10s
		.readTimeout = 30s
		.writeTimeout = 30s
		.weight = 8
		.keepAliveConns = 3
	}
}
